window.addEventListener('load', function () {
  var $owl = jQuery('#owl-gall_bigimg');
  var $lcp = jQuery('#owl-gall_bigimg-LCP');

  function revealCarousel() {
    if ($owl.hasClass('owl-loaded')) {
      $owl.addClass('show');
      $lcp.fadeOut(300, function(){ $lcp.remove(); });
      return true;
    }
    return false;
  }
  if (!revealCarousel()) {
    var tries = 0, timer = setInterval(function(){
      tries++;
      if (revealCarousel() || tries > 20) clearInterval(timer);
    }, 150);
  }
});
