        var marker = e.layer;
        var feature = marker.feature;

        // Создание HTML-контента для всплывающего окна
        var popupContent = '<div id="'+feature.properties.id+'" class="popup"><h6>'+feature.properties.title+'</h6><div class="image embed-responsive embed-responsive-16by9"><div class="embed-responsive-item" style="background-image:url(\''+feature.properties.img+'\');background-size:cover;" src="'+feature.properties.img+'"></div></div><div class="caption"><div class="truncate-text">'+feature.properties.description+'</div><div><a href="'+feature.properties.href+'" target="_blank" class="btn btn-sm btn-info mt-2 radius-0 text-white">Подробнее ...</a></div></div></div>';
        marker.bindPopup(popupContent, { closeButton: false, className: "custom-popup" });
    });

    // Установка geoHotels данных на слой
    myHotels.setGeoJSON(geoHotels);

    // Обработка кликов по элементам внутри всплывающих окон
    jQuery('#mapHotelId').on('click', '.popup .cycle a', function () {
        var slideshow = jQuery('.slideshow');
        var newSlide;

        if (jQuery(this).hasClass('prev')) {
            newSlide = slideshow.find('.active').prev();
            if (newSlide.index() < 0) {
                newSlide = jQuery('.image').last();
            }
        } else {
            newSlide = slideshow.find('.active').next();
            if (newSlide.index() < 0) {
                newSlide = jQuery('.image').first();
            }
        }

        slideshow.find('.active').removeClass('active').hide();
        newSlide.addClass('active').show();
        return false;
    });
 });
