document.addEventListener("DOMContentLoaded", function(event) {
 jQuery(".BoxData").on('click', '.BoxShowAddTours', function (e) {	
  if (jQuery(this).hasClass("collapsed")) {
   jQuery(this).find('span').text("Скрыть туры");
